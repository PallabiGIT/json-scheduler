	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>com.pallabi.scheduler.parent</groupId>
				<artifactId>scheduler-parent</artifactId>
				<version>0.0.1-SNAPSHOT</version>
				<type>pom</type>
				<scope>import</scope>
			</dependency>
		</dependencies>
	</dependencyManagement>
</project>
